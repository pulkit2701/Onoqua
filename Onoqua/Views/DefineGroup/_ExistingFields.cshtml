@model Onoqua.Entities.Models.Group
<div class="row">
    @if (Model != null)
    {<h3>@Model.GroupName</h3>
    @Html.HiddenFor(m => m.GroupID)

        <div class="row">
            <div class="col-md-6">
                @if (Model != null)
                {
                    foreach (var i in Model.GroupFields)
                    {
                        <h4>    @Html.LabelFor(m => i.Field.FieldName)</h4>
                    }
                }
            </div>
            <div class="col-md-6"></div>
        </div>
    }
</div>
